  "bugs": {
    "url": "https://github.com/georapbox/rss-feed-reader/issues"
  },
  "homepage": "https://github.com/georapbox/rss-feed-reader#readme",
  "devDependencies": {
    "@parcel/packager-raw-url": "~2.6.0",
    "@parcel/transformer-webmanifest": "~2.6.0",
    "eslint": "~8.17.0",
    "npm-run-all": "~4.1.5",
    "parcel": "~2.6.0",
    "process": "~0.11.10",
    "workbox-cli": "~6.5.3"
  },
  "dependencies": {
    "@georapbox/clipboard-copy-element": "~1.0.0",
